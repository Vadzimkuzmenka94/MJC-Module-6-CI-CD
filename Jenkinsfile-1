pipeline{
    agent any
    stages{
       stage('GetCode'){
            steps{
                git 'https://github.com/Vadzimkuzmenka94/module_6.git'
            }
         }
       stage('Test and SonarQube') {
             steps{
                withSonarQubeEnv('sonar-server') {
                    sh "./gradlew sonarqube"
                }
             }
         }
         }
